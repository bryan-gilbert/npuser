{
  "name": "npuser-server",
  "version": "1.0.0",
  "description": "No password auth services based on a TypeScript-Express boilerplate",
  "repository": {
    "type": "git",
    "url": "git+https:https://github.com/bryan-gilbert/npuser.git"
  },
  "keywords": [
    "TypeScript",
    "express",
    "nodejs",
    "boilerplate",
    "eslint"
  ],
  "author": "Sidhant Panda",
  "main": "dist/index.js",
  "scripts": {
    "eslint": "eslint src/server.ts",
    "postinstall": "chmod +x ./scripts/dev.sh",
    "build": "tsc",
    "lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts",
    "dev": "./scripts/dev.sh",
    "start": "node dist/src/server.js",
    "start:dev": "NODE_ENV=development nodemon -L  node src/server.ts",
    "prod:run": "cp .env.prod .env && docker-compose -f d-c-api.yml -f d-c-mongo.yml up --build",
    "test": "jest",
    "fix-lint": "./node_modules/eslint/bin/eslint.js  src --ext .ts --fix"
  },
  "dependencies": {
    "@hapi/joi": "17.1.1",
    "@types/glob": "^7.1.3",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-rate-limit": "^5.1.3",
    "glob": "^7.1.6",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.28.0",
    "nodemon": "2.0.4",
    "swagger-ui-express": "4.1.4",
    "winston": "3.3.2"
  },
  "devDependencies": {
    "@types/compression": "1.7.0",
    "@types/cors": "^2.8.7",
    "@types/dotenv": "8.2.0",
    "@types/hapi__joi": "^17.1.5",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "14.0.13",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "26.1.0",
    "jest-express": "1.12.0",
    "supertest": "4.0.2",
    "ts-jest": "26.1.1",
    "ts-node": "^9.0.0",
    "typescript": "3.9.5"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sidhantpanda/docker-express-typescript-boilerplate/issues"
  },
  "homepage": "https://github.com/sidhantpanda/docker-express-typescript-boilerplate#readme"
}
