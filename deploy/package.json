{
  "scripts": {
    "client:dev:build": "cp dev.env .env &&  npm run client:build",
    "client:dev:run": "cp dev.env .env &&  npm run client:run",
    "client:prod:build": "cp prod.env .env &&  npm run client:build",
    "client:prod:run": "cp prod.env .env && npm run client:run",
    "client:build": "docker-compose -f d-c-client.yml up --build",
    "client:run": "docker-compose -f d-c-client.yml up -d",
    "client:stop": "docker-compose -f d-c-client.yml down",
    "ls": "docker ps -a",
    "stop": "docker stop $(docker ps -a -q)",
    "rm": "docker rm $(docker ps -a -q)",
    "prune": "docker system prune",
    "final": ""
  },
  "scriptsComments": {
    "dev:*": "dev build or run (detacted) or stop",
    "prod:*": "prod build or run (detacted) or stop",
    "ls": "list docker containers",
    "stop": "stop all running containers",
    "rm": "remove all stopped containers",
    "prune": "delete everything not running, images, containers etc",
    "final": ""
  }
}
